{
  "manifest_version": 3,
  "name": "kureyonsintyan",
  "version": "1.0",
  "description": "クレヨンしんちゃんの拡張機能です！",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["renren.js"],
    "css": ["renren.css"]
  }],
  "web_accessible_resources": [
    {
      "resources": ["ketu.gif"],
      "matches": ["<all_urls>"]
    }
  ]
}